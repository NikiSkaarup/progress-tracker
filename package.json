{
	"name": "progress-tracker",
	"version": "0.0.2",
	"private": true,
	"prisma": {
		"seed": "node prisma/seed.js"
	},
	"scripts": {
		"start": "npx prisma migrate deploy && npx prisma generate && node build",
		"dev": "npx prisma generate && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"coolify-build": "vite build",
		"coolify-start": "npx prisma migrate deploy && npx prisma generate && node build/index.js"
	},
	"devDependencies": {
		"@auth/core": "^0.9.0",
		"@auth/prisma-adapter": "^1.0.0",
		"@auth/sveltekit": "^0.3.3",
		"@directus/sdk": "^10.3.3",
		"@melt-ui/svelte": "^0.16.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.0",
		"@sveltejs/kit": "^1.21.0",
		"@sveltejs/vite-plugin-svelte": "^2.4.2",
		"@sveltejs/vite-plugin-svelte-inspector": "^1.0.3",
		"@types/node": "^20.3.3",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.60.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"prisma": "^4.16.1",
		"svelte": "^4.0.1",
		"svelte-check": "^3.4.4",
		"svelte-exmarkdown": "^2.1.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.3.9"
	},
	"dependencies": {
		"@prisma/client": "^4.16.1"
	},
	"type": "module"
}
