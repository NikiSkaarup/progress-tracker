    "@sveltejs/kit": "^1.22.6",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@sveltejs/vite-plugin-svelte-inspector": "^1.0.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.4",
    "@types/better-sqlite3": "^7.6.4",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
