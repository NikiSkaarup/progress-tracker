    "@sveltejs/kit": "^1.24.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@sveltejs/vite-plugin-svelte-inspector": "^1.0.4",
    "@tailwindcss/container-queries": "^0.1.1",
