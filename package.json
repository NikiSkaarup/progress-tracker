    "prettier-plugin-tailwindcss": "^0.5.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-exmarkdown": "^2.1.0",
    "svelte-sequential-preprocessor": "^2.0.1",
